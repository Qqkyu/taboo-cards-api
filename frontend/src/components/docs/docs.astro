---
import CardModel from "./_components/card-model/card-model.astro";
import Endpoints from "./_components/endpoints/endpoints.astro";
import RateLimit from "./_components/rate-limit/rate-limit.astro";
import Section from "./_components/section/section.astro";
import BaseUrl from "./_components/base-url/base-url.astro";
---

<div class="flex w-[300px] flex-col sm:w-[500px] lg:w-[750px]">
  <Section id="card-model" header="Card model">
    <CardModel />
  </Section>
  <div class="divider"></div>
  <Section id="endpoints" header="Endpoints">
    <Endpoints />
  </Section>
  <div class="divider"></div>
  <Section id="base-url" header="Base URL">
    <BaseUrl />
  </Section>
  <div class="divider"></div>
  <Section id="rate-limit" header="Rate limit">
    <RateLimit />
  </Section>
</div>
