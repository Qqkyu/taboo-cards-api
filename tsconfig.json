{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "#server/*": ["./server/*"],
      "#src/*": ["./src/*"]
    },
    "baseUrl": "."
  },
  "extends": "astro/tsconfigs/base"
}
